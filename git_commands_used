  git branch main

  git checkout -b "feat/addButton"
  git add .
  git commit -m "Button added"

  #main, routing and feat/addButton are not merged
  #feat/addButton is ahead of routing
  #routing is ahead of main
  git checkout main
  git diff routing
  git merge routing
  

  git branch -M main
  git push -u origin main
  #equivalent to git push --set-upstream origin main
  #this allows git push or git pull later

  #without -u option you always need to specify remote branch

  git push origin main
  git pull origin main

  git branch -vv

  git show <hash>

